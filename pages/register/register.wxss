/* pages/register/register.wxss */
.logo-box{
  display: flex;
  justify-content: center;
}
.logo{
  width: 204rpx;
  margin-top: 123rpx;
}
.user-inputs{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  margin-top: 115rpx;
  
}
.user-input-userphone{
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0px;
	color: #cccccc;
  height: 90rpx;
}
.user-input-userphones{
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0px;
	color: #cccccc;
  border-bottom: 1rpx solid #eee;
  width: 450rpx;
  height: 90rpx;
}
.user-send-btn{
  position: absolute;
  bottom: 18rpx;
  width: 160rpx;
	height: 48rpx;
	border-radius: 10rpx;
	border: solid 1rpx #ff3300;
  right: 11rpx;
  font-family: PingFang-SC-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 48rpx;
	letter-spacing: 0px;
	color: #ff3300;
  text-align: center;
}
.coder-box{
  position: relative;
}
.done-btn{
  display: flex;
  justify-content: center;
}
.select-btn{
  width: 450rpx;
	height: 88rpx;
	background-color: #ff3300;
	border-radius: 44rpx;
  font-family: PingFang-SC-Medium;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #ffffff;
  text-align: center;
  line-height: 88rpx;
  margin-top: 53rpx;
}