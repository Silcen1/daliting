            </view> 
        </view> 
      </view> 
      <view class='counts-text'>
        <text class='number-counts'>共{{item.goods_list[0].num}}件商品</text> 
        <text class='counts-texts'>合计：￥{{item.total_price}}</text>
      </view>
      <view class='nav-store'>
         <button open-type="contact" class='open_type'>联系商家</button>
        <view class='orders' bindtap='status' data-order_id="{{item.order_id}}">去付款</view>
      </view>
    </view>
