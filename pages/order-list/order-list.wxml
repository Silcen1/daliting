<!--pages/order-list/order-list.wxml-->
<view class='order-item'> 
    <text class='{{type==0?"order-select":""}}' bindtap='order_item' data-id='0'>普通订单</text> 
    <text class='{{type==1?"order-select":""}}' >拼团订单</text>    <!--bindtap='order_item' data-id='1'-->
    <text class='{{type==2?"order-select":""}}' >砍价订单</text>  <!--bindtap='order_item' data-id='2'-->
    <text class='{{type==3?"order-select":""}}' >秒杀订单</text>    <!--bindtap='order_item' data-id='3'-->
</view>     
       
<view class='{{type==0?"order-item-btn":"up"}}' data-id='0'>
    <text class='{{item==0?"select-btn":""}}' bindtap='item' data-id='0'>全部</text>
    <text class='{{item==1?"select-btn":""}}' bindtap='item' data-id='1'>待付款</text>
    <text class='{{item==2?"select-btn":""}}' bindtap='item' data-id='2'>待发货</text>
    <text class='{{item==3?"select-btn":""}}' bindtap='item' data-id='3'>待收货</text>
    <text class='{{item==4?"select-btn":""}}' bindtap='item' data-id='4'>待评价</text>
</view> 
<view class='{{type==1?"order-item-btn":"up"}}' data-id='1'>
    <text class='{{order==0?"select-btn":""}}' bindtap='order' data-id='0'>全部</text>
    <text class='{{order==1?"select-btn":""}}' bindtap='order' data-id='1'>拼团中</text>
    <text class='{{order==2?"select-btn":""}}' bindtap='order' data-id='2'>拼团成功</text>
    <text class='{{order==3?"select-btn":""}}' bindtap='order' data-id='3'>拼团失败</text>
</view>
<view class='{{type==2?"order-item-btn":"up"}}' data-id='2'>
    <text class='{{btn==0?"select-btn":""}}' bindtap='btn' data-id='0'>全部</text>
    <text class='{{btn==1?"select-btn":""}}' bindtap='btn' data-id='1'>砍价中</text>
    <text class='{{btn==2?"select-btn":""}}' bindtap='btn' data-id='2'>砍价成功</text>
    <text class='{{btn==3?"select-btn":""}}' bindtap='btn' data-id='3'>砍价失败</text>
</view>
<view class='{{type==3?"order-item-btn":"up"}}' data-id='3'>
    <text class='{{odrer_item==0?"select-btn":""}}' bindtap='odrer_item' data-id='0'>全部</text>
    <text class='{{odrer_item==1?"select-btn":""}}' bindtap='odrer_item' data-id='1'>待付款</text>
    <text class='{{odrer_item==2?"select-btn":""}}' bindtap='odrer_item' data-id='2'>待发货</text>
     <text class='{{odrer_item==3?"select-btn":""}}' bindtap='odrer_item' data-id='3'>待收货</text>
    <text class='{{odrer_item==4?"select-btn":""}}' bindtap='odrer_item' data-id='4'>待评价</text>
</view>


