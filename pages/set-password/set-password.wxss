/* pages/set-password/set-password.wxss */
picure{
  width:100%;
  height:400rpx;
}
.picure image{
  width:200rpx;
  height: 160rpx;
  margin:130rpx 0 0 280rpx;
}
.contant{
  margin:80rpx 0 0 160rpx;
  width:460rpx;
  position: relative;
}
.contant input{
  font-size:26rpx;
  border-bottom:1rpx solid #eee;
  margin-bottom:60rpx;
}
.contant text{
  font-size:28rpx;
  color:red;
  width:145rpx;
  border:1rpx solid red;
  border-radius: 8rpx;
  position: absolute;
  top:120rpx;
  left:310rpx;
} 
.contant button{
  font-size:30rpx;
  color:#fff;
  background: red;
  border-radius: 60rpx;
}